<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.niclas.thirty.Result">

    <Button
        android:text="Continue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="bottom|right"
        android:id="@+id/continue_button"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="243dp"
        android:orientation="vertical">

        <ProgressBar
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/progressBar"
            android:max="10"
            android:visibility="visible"
            android:indeterminate="false"
            tools:visibility="visible"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

        <TableLayout
            android:layout_width="300dp"
            android:layout_height="220dp"
            android:gravity="bottom|center_horizontal"
            android:layout_marginBottom="10dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:orientation="vertical">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_margin="5dp"
                android:gravity="center_horizontal"
                >

                <TextView
                    android:text="Round 1: "
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:textColor="@android:color/black"/>

                <TextView
                    android:text="Result1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/result1"
                    android:textSize="20sp"
                    android:paddingLeft="15dp"
                    android:textColor="@android:color/holo_blue_dark"/>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_margin="5dp"
                android:gravity="center_horizontal"
                android:id="@+id/result_row2">

                <TextView
                    android:text="Round 2: "
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:textColor="@android:color/black"/>

                <TextView
                    android:text="Result2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/result2"
                    android:textSize="20sp"
                    android:paddingLeft="15dp"
                    android:textColor="@android:color/holo_blue_dark"/>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_margin="5dp"
                android:gravity="center_horizontal"
                android:id="@+id/result_row3">

                <TextView
                    android:text="Round 3:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView2"
                    android:textSize="14sp"
                    android:textColor="@android:color/black"/>

                <TextView
                    android:text="Result3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/result3"
                    android:textSize="20sp"
                    android:paddingLeft="15dp"
                    android:textColor="@android:color/holo_blue_dark"/>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_margin="5dp"
                android:gravity="center_horizontal"
                android:id="@+id/result_row4">

                <TextView
                    android:text="Round 4: "
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:textColor="@android:color/black"/>

                <TextView
                    android:text="Result4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/result4"
                    android:textSize="20sp"
                    android:paddingLeft="15dp"
                    android:textColor="@android:color/holo_blue_dark"/>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_margin="5dp"
                android:gravity="center_horizontal"
                android:id="@+id/result_row5">

                <TextView
                    android:text="Round 5: "
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:textColor="@android:color/black"/>

                <TextView
                    android:text="Result5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/result5"
                    android:textSize="20sp"
                    android:paddingLeft="15dp"
                    android:textColor="@android:color/holo_blue_dark"/>
            </TableRow>
        </TableLayout>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/result_row"
                android:layout_margin="5dp"
                android:gravity="center_horizontal">

                <TextView
                    android:text="Total:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:textSize="20sp"
                    android:textStyle="bold"/>

                <TextView
                    android:text="Result"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/result"
                    android:textSize="30sp"
                    android:textColor="@android:color/holo_blue_dark"
                    android:textStyle="bold"
                    android:paddingLeft="15dp"/>
            </TableRow>

            <TableLayout
                android:layout_width="300dp"
                android:layout_height="220dp"
                android:gravity="bottom|center_horizontal"
                android:layout_marginBottom="10dp"
                android:orientation="vertical"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:layout_margin="5dp"
                    android:gravity="center_horizontal"
                    android:id="@+id/result_row6">

                    <TextView
                        android:text="Round 6: "
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:textColor="@android:color/black"/>

                    <TextView
                        android:text="Result6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/result6"
                        android:textSize="20sp"
                        android:paddingLeft="15dp"
                        android:textColor="@android:color/holo_blue_dark"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:layout_margin="5dp"
                    android:gravity="center_horizontal"
                    android:id="@+id/result_row7">

                    <TextView
                        android:text="Round 7: "
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:textColor="@android:color/black"/>

                    <TextView
                        android:text="Result7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/result7"
                        android:textSize="20sp"
                        android:paddingLeft="15dp"
                        android:textColor="@android:color/holo_blue_dark"/>
                </TableRow>
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:layout_margin="5dp"
                    android:gravity="center_horizontal"
                    android:id="@+id/result_row8">

                    <TextView
                        android:text="Round 8: "
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:textColor="@android:color/black"/>

                    <TextView
                        android:text="Result8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/result8"
                        android:textSize="20sp"
                        android:paddingLeft="15dp"
                        android:textColor="@android:color/holo_blue_dark"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:layout_margin="5dp"
                    android:gravity="center_horizontal"
                    android:id="@+id/result_row9">

                    <TextView
                        android:text="Round 9: "
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:textColor="@android:color/black"/>

                    <TextView
                        android:text="Result9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/result9"
                        android:textSize="20sp"
                        android:paddingLeft="15dp"
                        android:textColor="@android:color/holo_blue_dark"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:layout_margin="5dp"
                    android:gravity="center_horizontal"
                    android:id="@+id/result_row10">

                    <TextView
                        android:text="Round 10:"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:textColor="@android:color/black"/>

                    <TextView
                        android:text="Result10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/result10"
                        android:textSize="20sp"
                        android:paddingLeft="15dp"
                        android:textColor="@android:color/holo_blue_dark"/>
                </TableRow>
            </TableLayout>

        </RelativeLayout>
    </LinearLayout>

</FrameLayout>
